    "@datev-research/mandat-shared-utils": "^1.0.2",
    "axios": "^1.7.7",
    "n3": "^1.23.1",
    "vue": "~3.4.38"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/n3": "^1.21.1",
